    z-index: -1;\r\n    filter: blur(2px) brightness(90%);\r\n}\r\n.body{\r\n    display:flex;\r\n    flex-direction: column;\r\n}\r\n\r\n/*page header*/\r\n.login_head{\r\n    display: flex;\r\n    flex-direction: column;\r\n    padding-top: 70px;\r\n    padding-left: 70px;\r\n}\r\n#login_netflix{\r\n    max-width: 256px;\r\n    height: auto;\r\n}\r\n#title_login{\r\n    font-family: 'Lato';\r\n    font-weight: bold;\r\n    font-size: 48px;\r\n    color: rgba(255, 255, 255, 0.9);\r\n}\r\n\r\n/*login*/\r\n.login_main{\r\n    display: flex;\r\n    flex-direction: column;\r\n    align-items: center;\r\n    padding-top:22px;\r\n}\r\n.login_comp{\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: center;\r\n    margin-bottom: 40px;\r\n}\r\n\r\n/*login::text*/\r\n.login_comp:nth-child(1){\r\n    flex-direction: row;\r\n    width: 380px;\r\n    justify-content: left;\r\n}\r\n.join_text{\r\n    margin-right: 0.3em;\r\n    font-weight: 700;\r\n    font-size: 18px;\r\n    color: rgba(255, 255, 255, 0.9);\r\n}\r\n.join_text:nth-child(2):hover{\r\n    transition-duration: 0.3s;\r\n    color: #E50914;\r\n    cursor: pointer;\r\n}\r\n\r\n/*login::input*/\r\n.login_btn{\r\n    margin-top: 6px;\r\n    margin-bottom: 6px;\r\n}\r\n.login_btn input{\r\n    width: 380px;\r\n    height: 65px;\r\n    border-radius: 5px;\r\n    border-style: none;\r\n    background-color: rgba(0,0,0, 0.8);\r\n    padding: 0px 27px;\r\n    font-weight: 500;\r\n    font-size: 15px;\r\n    color: white;\r\n}\r\n.login_btn input:focus{\r\n    outline: none;\r\n}\r\n.login_btn input::-webkit-input-placeholder{\r\n    text-align: center;\r\n}\r\n\r\n/*login::buttons*/\r\n#btn_submit{\r\n    width: 380px;\r\n    height: 65px;\r\n    border-radius: 5px;\r\n    border-style: none;\r\n    background-color: #E50914;\r\n    padding: 0px 27px;\r\n    font-weight: 700;\r\n    font-size: 15px;\r\n    color: white;\r\n}\r\n#btn_submit:hover{\r\n    transition-duration: 0.3s;\r\n    background-color: #cf0d17;\r\n    cursor: pointer;\r\n}\r\n#btn_submit:active{\r\n    background-color: #af0710;\r\n    cursor:progress;\r\n}\r\n#login_sub{\r\n    display: flex;\r\n    justify-content: space-between;\r\n    padding-top: 12px;\r\n    font-weight: 500;\r\n    font-size: 13px;\r\n    color: white;\r\n}\r\n\r\n/*login::buttons::checkbox*/\r\n#sub_left *{\r\n    margin-bottom: 3px;\r\n    cursor:pointer;\r\n}\r\n#sub_left div{\r\n    display: flex;\r\n    justify-content: flex-start;\r\n}\r\ninput[type=\"checkbox\"]{\r\n    display:none;\r\n}\r\ninput[type=\"checkbox\"] + label{\r\n    position: relative;\r\n    display:inline-block;\r\n}\r\ninput[type=\"checkbox\"] + label p{\r\n    margin-left: 20px;\r\n}\r\ninput[type=\"checkbox\"] + label:before {\r\n    content:'';\r\n    position: absolute;\r\n    border-radius: 2px;\r\n    width:15px;\r\n    height:15px;\r\n    background-color: rgba(0,0,0, 0.8);\r\n}\r\ninput[type=\"checkbox\"] + label img{\r\n    display:none;\r\n    position: absolute;\r\n    margin-left: -4px;\r\n    margin-top: -5px;\r\n    width: 25px;\r\n    cursor: pointer;\r\n\r\n}\r\ninput[type=\"checkbox\"]:checked + label img{\r\n    display: inline;\r\n}\r\n\r\n#facebook_logo{\r\n    width: 15px;\r\n    height: 15px;\r\n}\r\n#facebook_text{\r\n    margin-left: 7px;\r\n}\r\n\r\n#sub_right p{\r\n    font-weight: 700;\r\n}\r\n#sub_right p:hover{\r\n    transition-duration: 0.2s;\r\n    color: #E50914;\r\n    cursor: pointer;\r\n}\r\n\r\n/*page footer*/\r\n.login_btm{\r\n    display: flex;\r\n    justify-content: center;\r\n    margin-top: 30px;\r\n    margin-bottom: 70px;\r\n    font-weight: 400;\r\n    font-size: 15px;\r\n    color: white;\r\n}\r\n#btm_more{\r\n    margin-left: 5px;\r\n    font-weight: 700;\r\n    color: E50914;\r\n    cursor: pointer;\r\n}"},"lineCount":203}},"error":null,"hash":"332c93a911e2659f47833354090ba88c","cacheData":{"env":{}}}